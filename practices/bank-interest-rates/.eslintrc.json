{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/endOfLine": "off",
    "@next/next/no-img-element": "off",
    "@typescript-eslint/ban-types": "off",
    "prettier/prettier": "warn",
    "no-console": "warn",
    "no-var": "warn",
    "no-unused-vars": "warn",
    "quote-props": [
      "warn",
      "as-needed"
    ],
    "react-hooks/exhaustive-deps": "error"
  }
}
